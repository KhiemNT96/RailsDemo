<p id="notice"><%= notice %></p>

<h1 style="color:blue" align="center">USER SETTING</h1>

<table border="1px" align="center">
  <thead>
    <tr>
      <th style="color:green">ID</th>
      <th style="color:green">User Name</th>
      <th style="color:green">Email</th>
      <th style="color:green">Password</th>
      <th style="color:green">Comment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @userlogins.each do |userlogin| %>
      <tr>
        <td><%= userlogin.userid %></td>
        <td><%= userlogin.username %></td>
        <td><%= userlogin.useremail %></td>
        <td><%= userlogin.userpass %></td>
        <td><%= userlogin.comment %></td>
        <td><%= link_to 'Show', userlogin %></td>
        <td><%= link_to 'Edit', edit_userlogin_path(userlogin) %></td>
        <td><%= link_to 'Destroy', userlogin, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New user', new_userlogin_path %>
